      "parameters": {
        "jsCode": "// Extract AI response - NO SPLITTING, Conductor handles it!\nlet aiResponse;\nif ($input.item.json.output) {\n  aiResponse = $input.item.json.output;\n} else if ($input.item.json.text) {\n  aiResponse = $input.item.json.text;\n} else {\n  aiResponse = $input.item.json.response || 'Hello! How can I help you with your dispensary SMS marketing?';\n}\n\n// Get message_id from 'Prepare for AI' node\nconst prepareNode = $('Prepare for AI').item.json;\nconst phoneNumber = prepareNode.phone_number;\nconst messageId = prepareNode.message_id;\nconst conversationStage = prepareNode.conversation_stage;\n\nconsole.log('=== MarketBot Response (v4.107) ===');\nconsole.log('Phone:', phoneNumber);\nconsole.log('Message ID:', messageId);\nconsole.log('AI Response length:', aiResponse?.length, 'chars');\nconsole.log('Conductor will auto-split if needed!');\n\nif (!messageId) {\n  throw new Error('Message ID is missing!');\n}\n\n// Track lead qualification\nlet leadStatus = 'new';\nif (conversationStage === 'qualification') {\n  leadStatus = 'qualified';\n} else if (conversationStage === 'demo') {\n  leadStatus = 'demo_scheduled';\n}\n\nreturn {\n  json: {\n    phone_number: phoneNumber,\n    message_id: messageId,\n    message: aiResponse,\n    conversation_stage: conversationStage,\n    lead_status: leadStatus\n  }\n};"
      },
